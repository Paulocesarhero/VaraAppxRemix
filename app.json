{
  "expo": {
    "newArchEnabled": true,
    "name": "VaraAppx",
    "slug": "VaraAppx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E4F7E"
    },
    "ios": {
      "supportsTablet": true,
      "scheme": "VaraAppx",
      "bundleIdentifier": "com.paulocesarhero.VaraAppx"
    },
    "android": {
      "edgeToEdge": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#0E4F7E"
      },
      "package": "com.paulocesarhero.VaraAppx"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-sqlite",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "79c1f730-05c5-4362-99a3-04563b227be6"
      }
    }
  }
}
